{
  "multipart": [
    {"apply":  {"model":  "theatrical:block/cable/center"}},
    {
      "when": {"north":  true},
      "apply": {"model":  "theatrical:block/cable/connection"}
    },
    {   "when": { "east": true },
      "apply": { "model": "theatrical:block/cable/connection", "y": 90, "uvlock": false }
    },
    {   "when": { "south": true },
      "apply": { "model": "theatrical:block/cable/connection", "y": 180, "uvlock": false }
    },
    {   "when": { "west": true },
      "apply": { "model": "theatrical:block/cable/connection", "y": 270, "uvlock": false }
    },
    {   "when": { "up": true },
      "apply": { "model": "theatrical:block/cable/connection", "x": 270, "uvlock": false }
    },
    {   "when": { "down": true },
      "apply": { "model": "theatrical:block/cable/connection", "x": 90, "uvlock": false }
    }
  ]
}