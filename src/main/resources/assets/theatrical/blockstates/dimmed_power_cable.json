      },
      "apply": {
        "model": "theatrical:block/cable/dimmed_power/connection",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "theatrical:block/cable/dimmed_power/connection",
        "x": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "theatrical:block/cable/dimmed_power/connection",
        "x": 90,
        "uvlock": false
